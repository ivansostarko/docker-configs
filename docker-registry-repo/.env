# External URL clients use (Docker CLI, browsers)
REGISTRY_EXTERNAL_URL=https://registry.example.com
REGISTRY_TITLE=Company Registry

# Ports on the host
REGISTRY_HTTP_PORT=80
REGISTRY_HTTPS_PORT=443

# Logging
REGISTRY_LOG_LEVEL=info

# UI options
UI_DELETE_IMAGES=false

# Grafana (observability profile)
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=change-me
