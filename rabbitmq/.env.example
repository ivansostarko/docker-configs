RABBITMQ_NODENAME=rabbit@rabbitmq
RABBITMQ_DEFAULT_USER=admin

# Host ports (change if you have conflicts)
RABBITMQ_AMQP_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672
