# Core
BEANSTALKD_LISTEN_ADDR=0.0.0.0
BEANSTALKD_PORT=11300

# Publishing: keep localhost unless you know exactly why you're changing it
BEANSTALKD_PUBLISH_ADDR=127.0.0.1
BEANSTALKD_PUBLISH_PORT=11300

# Persistence (binlog)
BEANSTALKD_ENABLE_BINLOG=1
BEANSTALKD_BINLOG_DIR=/var/lib/beanstalkd/binlog
BEANSTALKD_FSYNC_PERIOD_SECONDS=1

# Limits
BEANSTALKD_MAX_JOB_SIZE=65535

# Console (optional)
BEANSTALKD_CONSOLE_PUBLISH_ADDR=127.0.0.1
BEANSTALKD_CONSOLE_PUBLISH_PORT=2080

# Exporter (optional)
BEANSTALKD_EXPORTER_PUBLISH_ADDR=127.0.0.1
BEANSTALKD_EXPORTER_PUBLISH_PORT=8080
BEANSTALKD_EXPORTER_TUBES=default

# Prometheus/Grafana (optional)
PROMETHEUS_PUBLISH_ADDR=127.0.0.1
PROMETHEUS_PUBLISH_PORT=9090
GRAFANA_PUBLISH_ADDR=127.0.0.1
GRAFANA_PUBLISH_PORT=3000
GRAFANA_ADMIN_USER=admin
