# Ports
MINDSDB_HTTP_PORT=47334
MINDSDB_MYSQL_PORT=47335
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000

# MindsDB APIs
MINDSDB_APIS=http,mysql

# MindsDB auth (change these)
MINDSDB_USERNAME=admin
MINDSDB_PASSWORD=CHANGE_ME_LONG_RANDOM
MINDSDB_HTTP_AUTH_TYPE=session_or_token

# Config DB (Postgres recommended).
POSTGRES_USER=mindsdb
POSTGRES_DB=mindsdb

# IMPORTANT: If you cannot accept credentials in plaintext, do NOT use this pattern.
# Option A (recommended for this stack): set the password here and keep .env secure.
# Option B: set MINDSDB_DB_CON="" to use local sqlite storage.
MINDSDB_DB_CON=postgresql://mindsdb:REPLACE_WITH_POSTGRES_PASSWORD@postgres:5432/mindsdb

# Logging
MINDSDB_LOG_LEVEL=INFO
MINDSDB_CONSOLE_LOG_LEVEL=INFO

# Avoid surprise GUI updates in prod
MINDSDB_GUI_AUTOUPDATE=false

# Optional default model keys
MINDSDB_DEFAULT_LLM_API_KEY=
MINDSDB_DEFAULT_EMBEDDING_MODEL_API_KEY=
MINDSDB_DEFAULT_RERANKING_MODEL_API_KEY=

# OTEL best-effort
OTEL_EXPORTER_TYPE=otlp

# Grafana
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=CHANGE_ME_TOO
