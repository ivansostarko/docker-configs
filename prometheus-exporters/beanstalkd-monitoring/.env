# Networks
MONITORING_NETWORK=monitoring
QUEUE_INTERNAL_NETWORK=queue_internal

# Exporter
BEANSTALKD_EXPORTER_PORT=8080

# If you are NOT running the local beanstalkd service (profile "stack"),
# set this to the reachable host:port from the exporter container.
BEANSTALKD_ADDRESS_FALLBACK=beanstalkd:11300

# Tube metrics
# WARNING: allTubes can create metric cardinality issues if you have many/dynamic tubes.
BEANSTALKD_ALL_TUBES=false
BEANSTALKD_TUBES=default

# Optional: reduce cardinality / scrape load (comma-separated metric names)
BEANSTALKD_SYSTEM_METRICS=
BEANSTALKD_TUBE_METRICS=

# Beanstalkd (only relevant if you use profile "stack")
BEANSTALKD_PORT=11300
BEANSTALKD_LOG_ENABLE=1
BEANSTALKD_LOG_DIR=/var/cache/beanstalkd
BEANSTALKD_LOG_SIZE=10485760
BEANSTALKD_FSYNC_INTERVAL=50
BEANSTALKD_JOB_MAX_SIZE=65535
BEANSTALKD_VERBOSE=0
