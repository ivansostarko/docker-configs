# Timezone
TZ=Europe/Zagreb

# Alertmanager publish port (host)
ALERTMANAGER_PORT=9093

# Logging/retention
ALERTMANAGER_LOG_LEVEL=info
ALERTMANAGER_DATA_RETENTION=120h

# If behind reverse proxy, set externally visible URL/path
ALERTMANAGER_EXTERNAL_URL=https://monitoring.example.com/alertmanager
ALERTMANAGER_ROUTE_PREFIX=/alertmanager
